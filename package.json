{
  "name": "@data-provider/axios",
  "version": "4.1.0",
  "description": "REST API Data Provider origin addon using Axios",
  "keywords": [
    "data-provider",
    "data-provider-addon",
    "data-provider-origin",
    "addon",
    "origin",
    "axios",
    "reactive",
    "data",
    "crud",
    "cache",
    "api",
    "rest"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "homepage": "https://www.data-provider.org",
  "repository": "https://github.com/data-provider/axios",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/data-provider"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "lint": "eslint src test *.js mocks",
    "lint-staged": "lint-staged",
    "mocks": "mocks-server",
    "mocks:silent": "mocks-server --log=silent",
    "build": "rollup --config",
    "stryker:run": "stryker run",
    "test": "jest --runInBand",
    "mocks-and-test": "start-server-and-test mocks tcp:127.0.0.1:3100 test",
    "test:unit": "npm run mocks-and-test",
    "test:mutation": "start-server-and-test mocks:silent tcp:127.0.0.1:3100 stryker:run",
    "prepare": "is-ci || husky install"
  },
  "peerDependencies": {
    "@data-provider/core": "3.x"
  },
  "dependencies": {
    "axios": "0.25.0",
    "axios-retry": "3.2.4",
    "path-to-regexp": "6.2.0",
    "query-string": "7.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.8",
    "@data-provider/core": "3.1.0",
    "@mocks-server/admin-api-paths": "2.2.0",
    "@mocks-server/main": "2.6.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@stryker-mutator/core": "5.6.0",
    "@stryker-mutator/jest-runner": "5.6.0",
    "babel-jest": "27.4.6",
    "babel-polyfill": "6.26.0",
    "cross-fetch": "3.1.4",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.4.7",
    "lint-staged": "12.1.7",
    "prettier": "2.5.1",
    "redux": "4.1.2",
    "rollup": "2.64.0",
    "rollup-plugin-terser": "7.0.2",
    "sinon": "12.0.1",
    "start-server-and-test": "1.14.0"
  },
  "lint-staged": {
    "*.js": "eslint",
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
