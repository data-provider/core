{
  "name": "@data-provider/browser-storage",
  "version": "2.0.3",
  "description": "Data Provider addon providing localStorage and sessionStorage origins",
  "keywords": [
    "data-provider",
    "addon",
    "origin",
    "browser",
    "localStorage",
    "sessionStorage",
    "reactive",
    "data",
    "crud"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "homepage": "https://www.data-provider.org",
  "repository": "https://github.com/data-provider/browser-storage",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "lint": "eslint src test",
    "lint-staged": "lint-staged",
    "build": "rollup --config",
    "test": "jest",
    "test:ci": "npm run test -- --coverage --ci --verbose=false --coverageReporters=lcov --coverageReporters=text-summary",
    "test:coverage": "npm run test:ci",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "peerDependencies": {
    "@data-provider/core": "2.x"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@data-provider/core": "2.1.2",
    "@rollup/plugin-commonjs": "11.1.0",
    "@rollup/plugin-json": "4.0.3",
    "@rollup/plugin-node-resolve": "7.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "25.4.0",
    "babel-polyfill": "6.26.0",
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest": "25.4.0",
    "lint-staged": "10.1.7",
    "prettier": "2.0.5",
    "redux": "4.0.5",
    "rollup": "2.7.2",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-uglify": "6.0.4",
    "sinon": "9.0.2"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "10.x || 12.x || 13.x"
  }
