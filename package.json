{
  "name": "@data-provider/browser-storage",
  "version": "2.0.5",
  "description": "Data Provider addon providing localStorage and sessionStorage origins",
  "keywords": [
    "data-provider",
    "addon",
    "origin",
    "browser",
    "localStorage",
    "sessionStorage",
    "reactive",
    "data",
    "crud"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "homepage": "https://www.data-provider.org",
  "repository": "https://github.com/data-provider/browser-storage",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/data-provider"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "lint": "eslint src test *.js",
    "lint-staged": "lint-staged",
    "build": "rollup --config",
    "test": "jest",
    "test:ci": "npm run test -- --coverage --ci --verbose=false --coverageReporters=lcov --coverageReporters=text-summary && npm run stryker",
    "test:coverage": "npm run test:ci",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "stryker:dev": "stryker run stryker.conf.local.js",
    "stryker": "stryker run"
  },
  "peerDependencies": {
    "@data-provider/core": "2.x"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@data-provider/core": "2.6.0",
    "@rollup/plugin-babel": "5.0.4",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "8.1.0",
    "@stryker-mutator/babel-transpiler": "3.3.1",
    "@stryker-mutator/core": "3.3.1",
    "@stryker-mutator/javascript-mutator": "3.3.1",
    "@stryker-mutator/jest-runner": "3.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.1.0",
    "babel-polyfill": "6.26.0",
    "coveralls": "3.0.9",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "redux": "4.0.5",
    "rollup": "2.18.1",
    "rollup-plugin-terser": "6.1.0",
    "sinon": "9.0.2"
  },
  "lint-staged": {
    "*.js": "eslint",
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 14.x"
  }
}
